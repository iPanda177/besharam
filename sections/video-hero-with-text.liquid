<script>
  const homePage_heroSectionVideoScripts = {
    videoSound: false,
    loadedVideo: false,
    video: document.querySelector("#hero-video"),
    btnPlay: document.querySelector("#btn-play"),
    btnPause: document.querySelector("#btn-pause"),
    btnMute: document.querySelector("#btn-nosound"),
    btnSound: document.querySelector("#btn-sound"),
    contentBlock: document.querySelector(".banner__box"),
    playerButtonWrapper: document.querySelector(".player__media"),
    init() {
      this.btnPlay.addEventListener("click", () => this.toggleVideo())
      this.btnPause.addEventListener("click", () => this.toggleVideo())
      this.btnMute.addEventListener("click", () => this.toggleSound())
      this.btnSound.addEventListener("click", () => this.toggleSound())
    },
    toggleVideo() {
    if (this.video.paused) {
      this.video.muted = !this.videoSound;
      this.video.play();
      this.contentBlock.classList.add("banner__box--hidden")
      this.playerButtonWrapper.style.opacity = 0;
      setTimeout(() => {
        this.playerButtonWrapper.classList.add("player__media--play")
        this.btnPlay.style.display = "none";
        this.btnPause.style.display = "block";
        this.playerButtonWrapper.style.opacity = 1;
      }, 300)
    } else {
      this.video.pause();
      this.contentBlock.classList.remove("banner__box--hidden")
      this.playerButtonWrapper.style.opacity = 0;
      setTimeout(() => {
        this.playerButtonWrapper.classList.remove("player__media--play")
        this.btnPlay.style.display = "block";
        this.btnPause.style.display = "none";
        this.playerButtonWrapper.style.opacity = 1;
      }, 300)
    }
  },
    toggleSound() {
    if (this.video.muted) {
      this.videoSound = true;
      this.btnSound.style.display = "block";
      this.btnMute.style.display = "none";
    } else {
      this.videoSound = false;
      this.btnSound.style.display = "none";
      this.btnMute.style.display = "block";
    }
    this.video.muted = !this.videoSound;
  }
  }
  document.addEventListener("DOMContentLoaded", () => {
    homePage_heroSectionVideoScripts.init()
  })
</script>

