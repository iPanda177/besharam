<div class="page-width">
  {% include 'breadcrumbs' %}
</div>

<div class="section--padding{% if section.settings.show_divider %} section--divider{% endif %}">
  <div class="page-width
    {% case section.settings.layout_width %}{% when 'full_width' %}page-width--full-width{% when 'layout_width_custom' %}page-width--hero-banner{% else %}{% endcase %}">
    <div id="Banner-{{ section.id }}" class="banner{% if section.settings.show_text_box %} banner--container{% endif %}{% if section.settings.show_text_below %} banner--mobile-bottom{% endif %}">
      <div class="banner__media-content">
        {%- if section.settings.subtitle != blank -%}
          <div class="banner__media-subtitle">
            {{ section.settings.subtitle }}
          </div>
        {%- endif -%}
        {%- if section.settings.main_title != blank -%}
          <p class="banner__media-title">
